{
	"name": "translator",
	"displayName": "translator",
	"description": "word translator",
	"publisher": "houxul",
	"version": "0.0.2",
	"repository": "https://github.com/houxul/translator",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.readAloud",
				"title": "朗读"
			}
		],
		"keybindings": [
			{
				"command": "extension.readAloud",
				"key": "f2",
				"mac": "f2",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.jumpWeb",
				"key": "f3",
				"mac": "f3"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.readAloud",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"ky": "^0.9.0",
		"ky-universal": "^0.1.0"
	},
	"icon": "images/translator.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	}
}
